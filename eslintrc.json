{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "jest"
  ],
  "rules": {
    "no-unused-vars": ["warn", { "args": "none" }]
  },
  "globals": {
    "tinymce": "readonly",
    "Sortable": "readonly"
  }
}